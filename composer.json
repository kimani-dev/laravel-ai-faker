{
    "name": "kimani/laravel-ai-faker",
    "version": "1.0.0",
    "description": "A Laravel package to generate realistic fake data using AI.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kimani\\LaravelAiFaker\\": "src/",
            "Kimani\\LaravelAiFaker\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "David Kimani",
            "email": "daviemim07@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "openai-php/client": "^0.10.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "orchestra/testbench": "^9.9"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
